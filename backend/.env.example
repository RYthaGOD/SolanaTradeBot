DRY_RUN_MODE=true

# API Keys (Optional)
DEEPSEEK_API_KEY=your_deepseek_key_here
MOBULA_API_KEY=your_mobula_key_here
MORALIS_API_KEY=your_moralis_key_here
JUPITER_API_KEY=your_jupiter_key_here
